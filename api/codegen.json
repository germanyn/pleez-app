{
  "overwrite": true,
  "schema": "http://localhost:4000",
  "generates": {
    "../types/graphql.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-mongodb"
      ]
    },
    "../types/graphql-resolvers.ts": {
      "plugins": [
        "typescript",
        "typescript-resolvers"
      ]
    },
    "./graphql.schema.json": {
      "plugins": [
        "introspection"
      ]
    }
  }
}